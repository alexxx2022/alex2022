K = int(input("Введите количество билетов:"))
A = [int(input("Введите возраст:")) for i in range(K)]
S = 0
for i in range(len(A)):
    if A[i] < 18:
        S = S + 0
    elif 18 <= A[i] < 25:
        S = S + 990
    elif A[i] >= 25:
        S = S + 1390
        if K > 3:
            S = S*0.9
print("Общая сумма билетов", int(S))
